<p id="notice"><%= notice %></p>

<h1>Profiles</h1>

<table>
  <thead>
    <tr>
      <th>Residence</th>
      <th>First time</th>
      <th>Expectancy</th>
      <th>Agile</th>
      <th>Agile description</th>
      <th>Hobbies</th>
      <th>Bring</th>
      <th>Proposal</th>
      <th>Bio</th>
      <th>Agilerelation</th>
      <th>Gender</th>
      <th>Size</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profiles.each do |profile| %>
      <tr>
        <td><%= profile.residence %></td>
        <td><%= profile.first_time %></td>
        <td><%= profile.expectancy %></td>
        <td><%= profile.agile_id %></td>
        <td><%= profile.agile_description %></td>
        <td><%= profile.hobbies %></td>
        <td><%= profile.bring %></td>
        <td><%= profile.proposal %></td>
        <td><%= profile.bio %></td>
        <td><%= profile.agileRelation_id %></td>
        <td><%= profile.gender_id %></td>
        <td><%= profile.size_id %></td>
        <td><%= link_to 'Show', profile %></td>
        <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
        <td><%= link_to 'Destroy', profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Profile', new_profile_path %>
